{
  "name": "inventory-mgmt",
  "version": "1.0.0",
  "private": true,
  "description": "NFC-Enabled Digital Inventory Management System",
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "build:standalone": "next build",
    "postinstall": "prisma generate",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "test:coverage": "jest --coverage",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:push": "prisma db push",
    "db:seed": "tsx lib/db/seeds/index.ts",
    "db:seed:dev": "NODE_ENV=development tsx lib/db/seeds/index.ts",
    "db:reset": "prisma db push --force-reset && npm run db:seed",
    "test:extensions": "tsx scripts/test-extensions.ts",
    "migrate:photos": "tsx scripts/execute-photo-migration.ts",
    "migrate:photos:dry-run": "tsx scripts/execute-photo-migration.ts -- --dry-run"
  },
